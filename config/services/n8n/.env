# n8n Configuration

# Server configuration
N8N_PORT=5678
N8N_HOST=n8n.osint.local
NODE_ENV=production

# Database configuration
DB_TYPE=postgresdb
DB_POSTGRESDB_HOST=supabase-db
DB_POSTGRESDB_PORT=5432
DB_POSTGRESDB_DATABASE=n8n
DB_POSTGRESDB_USER=n8n
DB_POSTGRESDB_PASSWORD=n8n123

# Security configuration
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=admin123

# Encryption configuration
N8N_ENCRYPTION_KEY=supersecretencryptionkey

# Execution configuration
EXECUTIONS_PROCESS=main
EXECUTIONS_MODE=regular

# Queue configuration
QUEUE_HEALTH_CHECK_ACTIVE=true
QUEUE_RECOVERY_INTERVAL=60

# External hooks configuration
EXTERNAL_HOOK_FILES=/home/node/.n8n/externalHooks.js

# Templates configuration
TEMPLATES_ENABLED=true
TEMPLATES_HOST=localhost
TEMPLATES_PORT=5679

# Public API configuration
N8N_PUBLIC_API_DISABLED=false
N8N_PUBLIC_API_SWAGGERUI_DISABLED=false

# Logging configuration
N8N_LOG_LEVEL=info
N8N_LOG_OUTPUT=console
N8N_LOG_FILE_LOCATION=/home/node/.n8n/logs

# Binary data configuration
N8N_BINARY_DATA_MODE=default

# Timezone configuration
GENERIC_TIMEZONE=America/New_York

# Diagnostics configuration
N8N_DIAGNOSTICS_ENABLED=true
N8N_DIAGNOSTICS_CONFIG_FRONTEND=1
N8N_DIAGNOSTICS_CONFIG_BACKEND=1
N8N_DIAGNOSTICS_CONFIG_SERVER=1